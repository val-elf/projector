{
	"compilerOptions": {
	  "allowJs": true,
	  "target": "es2017",
	  "module": "esnext",
	  "moduleResolution": "node",
	  "sourceMap": true,
	  "emitDecoratorMetadata": true,
	  "experimentalDecorators": true,
	  "downlevelIteration": true,
	  "lib": [
		"es2018",
		"dom"
	  ],
	  "noUnusedLocals": true,
	  "typeRoots": [
		"./node_modules/@types",
		"./typings"
	  ],
	  "importHelpers": true,
	  "skipLibCheck": true,
	  "baseUrl": ".",
	  "rootDir": ".",
	  "paths": {
		"~/*": [
		  "./*",
		  "src/app/*"
		]
	  }
	},
	"awesomeTypescriptLoaderOptions": {
	  "useCache": true
	}
  }