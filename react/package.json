{
  "version": "1.0.0",
  "name": "projector2",
  "watch": {
    "start:dev": [
      "backend/**/*.js",
      "server/**/*.js",
      "app.js"
    ],
    "start:prod": [
      "backend/**/*.js",
      "server/**/*.js",
      "app.js"
    ],
    "start:dist": [
      "backend/**/*.js",
      "server/**/*.js",
      "app.js"
    ]
  },
  "scripts": {
    "start:dev": "node app.js --dev",
    "start:dist": "node app.js --dist",
    "start:prod": "node app.js",
    "watch": "npm-watch start:dev",
    "watch:dist": "npm-watch start:dist",
    "watch:prod": "npm-watch start:prod",
    "clean": "rimraf dist",
    "start": "npm run start:dev",
    "build:dev": "npm run clean && webpack --config webpack.config.dev.js",
    "build:prod": "npm run clean && webpack --config webpack.config.prod.js",
    "build": "npm run build:prod"
  },
  "devDependencies": {
