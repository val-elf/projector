    "@mui/material": "^5.8.2",
    "@uirouter/react": "^1.0.7",
    "axios": "^0.27.2",
    "base-64": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.28.0",
    "dom-helpers": "^5.2.1",
    "express": "^4.18.1",
    "http-proxy": "^1.18.1",
    "i18next": "^21.8.8",
    "jshint": "^2.13.4",
    "moment": "^2.29.3",
    "picture-editor": "github:val-elf/picture-editor",
    "projector-localizations": "^0.0.2",
    "raphael": "^2.3.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-templates": "^0.6.3",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redux": "^4.2.0",
    "redux-i18n": "^1.5.24",
    "svg.js": "^2.7.1",
    "tinymce": "^6.0.3",
    "utf8": "^3.0.0"
