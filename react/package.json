    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.0.4",
    "less-loader": "^3.0.0",
    "material-ui-pickers": "^1.0.0-rc.17",
    "material-ui-time-picker": "^0.1.7",
    "moment": "^2.18.1",
    "muicss": "^0.9.35",
    "node-less": "^1.0.0",
    "raphael": "^2.2.7",
    "react": "^16.5.2",
    "react-addons-linked-state-mixin": "^0.14.5",
    "react-bootstrap": "^0.32.1",
    "react-cookie": "^1.0.4",
    "react-dom": "^16.3.2",
    "react-material-design": "^0.4.3",
    "react-redux": "^5.0.6",
    "react-templates": "^0.6.3",
    "react-templates-loader": "^0.6.1",
