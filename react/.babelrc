{
	"presets": [
		"es2015",
		"react",
		"stage-0"
	],
	"plugins": [
		[
			"babel-plugin-root-import",
			"babel-plugin-transform-builtin-extend"
		]
	],
	"env": {
		"production": {
			"presets": [
				"es2015",
				"stage-0"
			],
			"plugins": [
				"babel-plugin-root-import",
				[
					"babel-plugin-transform-builtin-extend",
					{
						"globals": [
							"Array"
						]
					}
				]
			]
		},
		"development": {
			"presets": [
				"react-hmre",
				"es2015",
				"stage-0"
			],
			"plugins": [
				[
					"babel-plugin-transform-builtin-extend",
					{
						"globals": [
							"Array"
						]
					}
				]
			]
		}
	}
}