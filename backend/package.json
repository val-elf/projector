{
  "version": "1.0.0",
  "name": "projector2",
  "scripts": {
    "start:dev": "node --enable-source-maps dist/app.js --dev",
    "start:dist": "node --enable-source-maps dist/app.js --dist",
    "start:prod": "node --enable-source-maps dist/app.js",
    "start:test": "node --enable-source-maps dist/app.js --test",
    "start:swagger": "npm run build && node --enable-source-maps dist/swagger.js",
    "start:jsdoc:test": "npm run build && npm run run:jsdoc:test",
    "watch": "npm run clean && nodemon -e ts --exec \"npm run build && npm run start:dev",
    "watch:test": "nodemon -e ts --exec \"npm run test",
    "watch:jsdoc:test": "npm run clean && nodemon -e ts --exec \"npm run start:jsdoc:test",
    "clean": "rimraf dist",
    "start": "npm run build && npm run start:dev",
    "build": "tsc && tsc-alias",
    "test": "npm run build && npm run start:test",
    "postinstall": "link-module-alias",
    "run:jsdoc:test": "cls && node --enable-source-maps dist/openapi/tests/tests.js --test --verbose=true"
  },
  "devDependencies": {
    "@types/md5": "^2.3.2",
    "@types/node": "^18.14.1",
    "base-64": "^0.1.0",
    "link-module-alias": "^1.2.0",
    "nodemon": "^2.0.20",
    "rimraf": "^2.5.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4",
    "utf8": "^3.0.0"
  },
  "dependencies": {
    "@types/express": "^4.17.16",
    "@types/express-session": "^1.17.5",
    "@types/swagger-jsdoc": "^6.0.1",
    "colors": "^1.4.0",
    "cookie-parser": "^1.3.5",
    "dom-helpers": "^3.3.1",
    "exiftool": "0.0.3",
    "express": "^4.13.3",
    "express-request-context": "^1.0.1",
    "express-session": "^1.11.3",
    "glob": "^9.3.4",
    "jimp": "^0.16.2",
    "jshint": "^2.9.6",
    "json5": "^2.2.3",
    "mammoth": "^1.4.9",
    "md5": "^2.0.0",
    "mongodb": "^5.1.0",
    "mongoose": "^6.9.2",
    "node-gyp": "^9.3.1",
    "npm-watch": "^0.11.0",
    "rxjs": "^7.8.0",
    "sharp": "^0.30.6",
    "source-map-support": "^0.5.21",
    "stream-transcoder": "0.0.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/dist/**"
    ],
    "delay": 100
  }
}
