{
  "version": "1.0.0",
  "name": "projector2",
  "watch": {
    "start:dev": [
      "./dist/**/*.ts"
    ],
    "start:prod": [
      "./src/**/*.ts"
    ],
    "start:dist": [
      "./src/**/*.ts"
    ]
  },
  "scripts": {
    "start:dev": "tsc && tsc-alias && node --enable-source-maps dist/app.js --dev",
    "start:dist": "node --enable-source-maps dist/app.js --dist",
    "start:prod": "node --enable-source-maps dist/app.js",
    "watch": "nodemon -e ts --exec \"npm run start:dev",
    "watch:dev": "tsc --watch && tsc-alias",
    "watch:dist": "npm-watch start:dist",
    "watch:prod": "npm-watch start:prod",
    "clean": "rimraf dist",
    "start": "npm run start:dev",
    "build": "tsc && tsc alias",
    "postinstall": "link-module-alias"
  },
  "devDependencies": {
    "@types/md5": "^2.3.2",
    "base-64": "^0.1.0",
    "link-module-alias": "^1.2.0",
    "nodemon": "^2.0.20",
    "rimraf": "^2.5.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4",
    "utf8": "^3.0.0"
  },
  "dependencies": {
    "@types/express": "^4.17.16",
    "@types/express-session": "^1.17.5",
    "cookie-parser": "^1.3.5",
    "dom-helpers": "^3.3.1",
    "exiftool": "0.0.3",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "jimp": "^0.16.2",
    "jshint": "^2.9.6",
    "mammoth": "^1.4.9",
    "md5": "^2.0.0",
    "mongodb": "^3.4.1",
    "mongoose": "^5.8.3",
    "node-gyp": "^9.3.1",
    "npm-watch": "^0.11.0",
    "rxjs": "^7.8.0",
    "sharp": "^0.30.6",
    "stream-transcoder": "0.0.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/dist/**"
    ],
    "delay": 100
  }
}
